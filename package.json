{
  "name": "initiative-ui2",
  "version": "0.0.8",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/initiative-ui2/initiative-ui2.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --watch --serve",
    "test": "stencil test --spec --e2e --coverage",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook-temp": "start-storybook -s ./src/stories/assets -p 6012",
    "build-storybook-temp": "build-storybook",
    "storybook.run": "start-storybook -p 6012 -s ./src/stories/assets",
    "storybook": "npm-run-all --parallel build storybook.run",
    "build-storybook": "build-storybook -c ./.storybook -o ../public"
  },
  "dependencies": {
    "@shoelace-style/shoelace": "^2.0.0-beta.22",
    "@stencil/core": "2.3.0",
    "@stencil/sass": "^1.3.2",
    "clsx": "^1.1.1",
    "flatpickr": "^4.6.6",
    "node-sass": "^5.0.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-syntax-jsx": "^7.12.1",
    "@babel/plugin-transform-react-jsx": "^7.12.7",
    "@stencil/react-output-target": "0.0.9",
    "@storybook/addon-actions": "^6.1.10",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/addon-knobs": "^6.1.10",
    "@storybook/addon-links": "^6.1.10",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addon-viewport": "^6.1.11",
    "@storybook/html": "^6.1.10",
    "@storybook/preset-scss": "^1.0.3",
    "@types/jest": "^26.0.12",
    "@types/puppeteer": "^3.0.1",
    "babel-loader": "^8.2.2",
    "case": "^1.6.3",
    "copy-webpack-plugin": "^6.3.2",
    "css-loader": "^5.0.1",
    "jest": "^26.4.2",
    "jest-cli": "^26.4.2",
    "jsx-dom": "^6.4.23",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^5.2.1",
    "sass-loader": "^10.1.0",
    "storybook-design-token": "^0.8.1",
    "style-loader": "^2.0.0",
    "write-file-webpack-plugin": "^4.5.1"
  }
}
